<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="programBack" prefHeight="600.0" prefWidth="360.0" stylesheets="@stylish.css" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.newmsg.MessagePageController">
    <HBox fx:id="topBar" alignment="CENTER" layoutX="2.0" prefHeight="69.0" prefWidth="360.0">
        <Text fx:id="title" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Nimble Natter Network">
            <font>
                <Font name="Old English Text MT" size="18.0" />
            </font>
        </Text>
    </HBox>

    <TextField fx:id="messageField" layoutX="14.0" layoutY="561.0" prefHeight="25.0" prefWidth="282.0" />
    <Button fx:id="Sender" layoutX="302.0" layoutY="561.0" mnemonicParsing="false" onAction="#sendMessage" text="Send" />

    <ListView fx:id="messageListView" layoutX="15.0" layoutY="80.0" prefHeight="473.0" prefWidth="332.0" />

</AnchorPane>
